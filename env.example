# Supabase (from https://supabase.com/dashboard → Project → API)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Order seeding script (npm run seed:orders)
# Set ENABLE_SEEDING=true to run the seeder; leave false or omit to disable.
ENABLE_SEEDING=false
# Tick interval in milliseconds (e.g. 60000 = 1 minute). Minimum 5000.
SEED_TICK_INTERVAL_MS=60000
